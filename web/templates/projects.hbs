{{#partial page}}
<div class="container">
  <div class="row">
    <div class="col-xs-12">
      <div class="page-header">
        <h1>Listing Projects</h1>
      </div>

      <table class="table">
        <thead>
          <tr>
            <th>Name</th>
            <th>Start</th>
            <th>End</th>
            <th>Sensor1 name</th>
            <th>Sensor2 name</th>
            <th colspan="3"></th>
          </tr>
        </thead>

        <tbody>
          {{#each projects}}
            <tr>
              <td>{{name}}</td>
              <td>{{start}}</td>
              <td>{{end}}</td>
              <td>{{sensor1_name}}</td>
              <td>{{sensor2_name}}</td>
              <td> <a href="/projects/{{id}}" class="btn btn-default">Show</a> </td>
              <td> <a href="/projects/{{id}}/edit" class="btn btn-default">Edit</a> </td>
              <td> </td>
            </tr>
          {{/each}}
        </tbody>
      </table>

      <br>

      <a href="/projects/new" class="btn btn-primary">New Project</a>

    </div>
  </di>
</div>
{{/partial}}
{{~> layout~}}
